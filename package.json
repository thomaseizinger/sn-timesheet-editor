{
  "name": "standard-notes-timesheets",
  "version": "0.1.0",
  "description": "Maintain timesheets with Standard Notes",
  "keywords": [
    "Standard Notes",
    "Standard Notes Extensions"
  ],
  "private": true,
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/standardnotes/editor-template-cra-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/standardnotes/editor-template-cra-typescript/issues"
  },
  "sn": {
    "main": "build/index.html"
  },
  "homepage": ".",
  "scripts": {
    "start": "PORT=8001 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "serve": "http-server ./build -p 3000 --cors",
    "pretty": "prettier --write 'src/**/*.{html,css,scss,js,jsx,ts,tsx,json}' README.md",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "README.md": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@js-joda/core": "^5.2.0",
    "@standardnotes/editor-kit": "2.2.5",
    "@standardnotes/stylekit": "5.29.3",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "framer-motion": "^6.3.15",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "node-sass": "7.0.1",
    "prettier": "^2.7.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.3",
    "usehooks-ts": "^2.6.0"
  },
  "dependencies": {}
}
